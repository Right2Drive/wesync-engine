{
  "name": "wesync-engine",
  "version": "0.0.1",
  "description": "Video sync engine designed for WeSync platform",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "git@github.com:Right2Drive/wesync-engine.git",
  "author": "Right2Drive",
  "license": "GPL-3.0",
  "private": false,
  "scripts": {
    "clean": "yarn clean:dist",
    "clean:dist": "yarn rimraf dist/",
    "clean:dist:test": "yarn rimraf dist/test",
    "lint": "yarn lint:src && yarn lint:test",
    "lint:src": "yarn tslint \"src/**/*.ts\"",
    "lint:test": "yarn tslint \"test/**/*.ts\"",
    "test": "yarn mocha ./test/**/*.spec.ts",
    "watch": "yarn build --watch",
    "build": "yarn build:dev",
    "build:dev": "yarn clean && tsc && yarn clean:dist:test",
    "build:prod": "yarn build:dev && echo \"TODO: UGLIFY ME PLS\"",
    "webpack:build": "yarn webpack && yarn clean:declarations:test && yarn copy:declarations && yarn clean:declarations",
    "tslint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.4",
    "@types/webtorrent": "^0.98.4",
    "chai": "^4.1.2",
    "copyfiles": "^2.0.0",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "testdouble": "^3.8.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "webtorrent": "^0.100.0"
  }
