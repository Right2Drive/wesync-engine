{
  "name": "wesync-engine",
  "version": "0.0.1",
  "description": "Video sync engine designed for WeSync platform",
  "main": "dist/wesync-engine.js",
  "repository": "git@github.com:Right2Drive/wesync-engine.git",
  "author": "Right2Drive",
  "license": "GPL-3.0",
  "private": false,
  "scripts": {
    "clean": "yarn clean:dist",
    "clean:dist": "yarn rimraf dist/",
    "clean:declarations": "yarn rimraf dist/declarations",
    "clean:declarations:test": "yarn rimraf dist/declarations/test",
    "copy:declarations": "yarn copyfiles -u 3 \"dist/declarations/src/**/*\" dist/",
    "lint": "yarn tslint -c tslint.json -p tsconfig.json \"src/**/*.ts\"",
    "test": "yarn mocha ./test/**/*.spec.ts",
    "watch": "yarn build --watch",
    "build": "yarn build:dev",
    "build:dev": "yarn clean && yarn cross-env WEBPACK_ENV=dev yarn webpack:build",
    "build:prod": "yarn clean && yarn cross-env WEBPACK_ENV=prod yarn webpack:build",
    "cross-env": "./node_modules/.bin/cross-env",
    "webpack:build": "yarn webpack && yarn clean:declarations:test && yarn copy:declarations && yarn clean:declarations",
    "webpack": "./node_modules/.bin/webpack",
    "rimraf": "./node_modules/.bin/rimraf",
    "tslint": "./node_modules/.bin/tslint --project tsconfig.json",
    "mocha": "./node_modules/.bin/mocha"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.4",
    "@types/webtorrent": "^0.98.4",
    "babel-loader": "^8.0.0-beta",
    "chai": "^4.1.2",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.2.0",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "testdouble": "^3.8.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.3",
    "webpack": "^4.12.0",
    "webpack-command": "^0.2.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.51",
    "webtorrent": "^0.100.0"
  }
}
